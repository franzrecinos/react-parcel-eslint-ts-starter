{
  "name": "react-parcel-eslint-ts-starter",
  "version": "1.0.0",
  "description": "A React starter with EsLint TypeScript Parcel",
  "main": "index.tsx",
  "repository": "git@github.com:franzrecinos/redux-logic-antd.git",
  "author": "Franz Recinos",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "parcel build src/index.html",
    "lint": "run-p -n lint:tsc lint:tsx",
    "lint:tsc": "tsc --noEmit",
    "lint:tsx": "eslint src --ext .ts,.tsx",
    "start": "parcel src/index.html --https --open",
    "dev": "parcel src/index.html --log-level 1 --https --open",
    "semantic-release": "semantic-release -e ./config/release.config.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/dom": "^5.2.0",
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.14",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.13.0",
    "codecov": "^3.5.0",
    "conventional-changelog-eslint": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "semantic-release": "^17.2.3",
    "sr-commit-analyzer": "^2.0.3",
    "sr-release-notes-generator": "^1.1.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
